target build-lambdas {
    run build-lambda-base
    run build-lambda {
        var {LAMBDA list-id-providers }
    }
    run build-lambda {
        var {LAMBDA post-auth }
    }
    run build-lambda {
        var {LAMBDA pre-signup }
    }
    run build-lambda {
        var {LAMBDA pre-token }
    }
    run build-lambda {
        var {LAMBDA register-user }
    }
}